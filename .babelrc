{
  "env": {
    "test": {
      "presets": [["@babel/preset-env", { "targets": { "node": "current" } }]],
      "plugins": ["@babel/plugin-proposal-class-properties"],
    },
    "development": {
      "presets": [["@babel/preset-env", { "targets": { "node": "current", "browsers": "> 1%, not IE 9" } }]],
      "plugins": ["@babel/plugin-proposal-class-properties"],
    },
    "production": {
      "presets": [["@babel/preset-env", { "targets": { "node": "current", "browsers": "> 1%, not IE 9" } }]],
      "plugins": ["@babel/plugin-proposal-class-properties"],
    }
  }
}
